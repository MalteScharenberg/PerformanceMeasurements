{
  "evaluator_behaviors": [
    {
      "type": "Throughput",
      "matlab_export": true,
      "arguments": {
        "node_id": 1,
        "block_size": 10
      }
    },
    {
      "type": "Delay",
      "matlab_export": false,
      "arguments": {
        "node_id": 1,
        "block_size": 10
      }
    }
  ],
  "matlab_exporter": {
    "file": "results.mat"
  },
  "nodes": [
    {
      "behaviors": [
        {
          "type": "Source",
          "arguments": {
            "quantity": 100,
            "payload": 100,
            "dest": 1,
            "ack": 1
          }
        }
      ],
      "hardware": {
        "type": "HardwareMock",
        "arguments": {
          "port": 1
        }
      }
    },
    {
      "behaviors": [
        {
          "type": "Sink",
          "arguments": {
          }
        }
      ],
      "hardware": {
        "type": "HardwareMock",
        "arguments": {
          "port": 2
        }
      }
    }
  ]
}