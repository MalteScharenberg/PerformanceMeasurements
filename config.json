{
  "evaluator_behaviors": [
    {
      "type": "Throughput",
      "matlab_export": true,
      "arguments": {
        "node_id": 1,
        "block_size": 100
      }
    },
    {
      "type": "Delay",
      "matlab_export": true,
      "arguments": {
        "node_id": 1,
        "block_size": 20,
        "group_by": "payload"
      }
    },
    {
      "type": "Loss",
      "matlab_export": true,
      "arguments": {
        "node_id": 1
      }
    }
  ],
  "matlab_exporter": {
    "file": "results.mat"
  },
  "nodes": [
    {
      "behaviors": [
        {
          "type": "Source",
          "arguments": {
            "quantity": 200,
            "increase_payload": true,
            "payload": 100,
            "dest": 99,
            "ack": true,
            "full_buffer": false
          }
        }
      ],
      "hardware": {
        "type": "XBeeWrapper",
        "arguments": {
          "port": "/dev/tty.usbserial-DA0121K1",
          "address": 10
        }
      }
    },
    {
      "behaviors": [
        {
          "type": "Sink",
          "arguments": {
          }
        }
      ],
      "hardware": {
        "type": "XBeeWrapper",
        "arguments": {
          "port": "/dev/tty.usbserial-DA0121KA",
          "address": 11
        }
      }
    }
  ]
}